record(mbbo, "$(BASE):SP_TYPE") {
  field(DTYP, "stream")
  field(OUT, "@agilent.proto writenum(101) $(PORT)")
  field(ZRST, "Frequency")
  field(ONST, "Power")
  field(TWST, "Time")
  field(THST, "Normal")              # What the hell is "NORMAL"?!?
  field(FRST, "Pressure")            # Units for pressure?
  field(ZRVL, "0")
  field(ONVL, "1")
  field(TWVL, "2")
  field(THVL, "3")
  field(FRVL, "4")
  field(FLNK, "$(BASE):SP_CALCDIS")
  info(autosaveFields, "VAL")
}

record(calc, "$(BASE):SP_CALCDIS") {
  field(INPA, "$(BASE):SP_TYPE NPP")
  field(CALC, "1<<A")
  field(FLNK, "$(BASE):SP_DIS")
}

record(mbboDirect, "$(BASE):SP_DIS") {
  field(OMSL, "closed_loop")
  field(DOL, "$(BASE):SP_CALCDIS NPP")
  field(FLNK, "$(BASE):SP_WRITEVAL")
}

