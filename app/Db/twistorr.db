record(bo, "$(BASE):RUN") {
  field(DTYP, "stream")
  field(OUT, "@agilent.proto writelog(000) $(PORT)")
  field(ZNAM, "Stop")
  field(ONAM, "Start")
  info(autosaveFields, "VAL")
}

